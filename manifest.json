{
  "manifest_version": 3,
  "name": "Bandwidth Hero MV3",
  "description": "Compress images via your proxy to save bandwidth and data.",
  "version": "0.6.4",
  "permissions": ["storage", "declarativeNetRequest", "declarativeNetRequestFeedback"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "minimum_chrome_version": "100"
}
